    <script>

        // Pagination Start ---------------------------------------------------------------------------------------
        window.onload = function () {
            getPageCount(0, false, false);
        }

        // ********************************************************************************************************
        const pageNumbers = (total, max, current) => {
            const half = Math.floor(max / 2);
            let to = max;

            if (current + half >= total) {
                to = total;
            } else if (current > half) {
                to = current + half;
            }

            let from = to - max;
            return Array.from({ length: max }, (_, i) => (i + 1) + from);
        }

        // ********************************************************************************************************
        function PaginationButton(totalPages, maxPagesVisible = 5, currentPage = 1) {
            let pages = pageNumbers(totalPages, maxPagesVisible, currentPage);
            //console.log('fourth get pages: ' + pages)
            let currentPageBtn = null;
            const buttons = new Map();
            const disabled = {
                start: () => pages[0] === 1,
                prev: () => currentPage === 1,
                end: () => pages.slice(-1)[0] === totalPages,
                next: () => currentPage === totalPages
            }
            const frag = document.createDocumentFragment();
            const paginationButtonContainer = document.createElement('div');
            paginationButtonContainer.className = 'dataTables_paginate paging_simple_numbers';

            //   default page loading 
            paginationButtonContainer.value = currentPage;
            filterStocks(currentPage); 

            const createAndSetupButton = (label = '', cls = '', disabled = false, handleClick) => {
                const buttonElement = document.createElement('button');
                if (label == 'prev') {
                    buttonElement.innerHTML = '<i class="ri-arrow-left-s-line"></i>';
                }
                else if (label == 'next') {
                    buttonElement.innerHTML = '<i class="ri-arrow-right-s-line"></i>';
                }
                else if (label == 'start') {
                    buttonElement.innerHTML = '<i class="fas fa-angle-double-left"></i>';
                }
                else if (label == 'end') {
                    buttonElement.innerHTML = '<i class="fas fa-angle-double-right"></i>';
                }
                else {
                    buttonElement.textContent = label;
                }

                buttonElement.className = `page-btn ${cls}`;
                buttonElement.disabled = disabled;
                buttonElement.addEventListener('click', e => {
                    handleClick(e);
                    this.update();
                    paginationButtonContainer.value = currentPage;
                    filterStocks(currentPage);
                    paginationButtonContainer.dispatchEvent(new Event('change'));
                });

                return buttonElement;
            }

            const onPageButtonClick = e => currentPage = Number(e.currentTarget.textContent);

            const onPageButtonUpdate = index => (btn) => {
                btn.textContent = pages[index];

                if (pages[index] === currentPage) {
                    currentPageBtn.classList.remove('active');
                    btn.classList.add('active');
                    currentPageBtn = btn;
                    currentPageBtn.focus();
                }
            };

            buttons.set(
                createAndSetupButton('start', 'start-page', disabled.start(), () => currentPage = 1),
                (btn) => btn.disabled = disabled.start()
            )

            buttons.set(
                createAndSetupButton('prev', 'prev-page', disabled.prev(), () => currentPage -= 1),
                (btn) => btn.disabled = disabled.prev()
            )

            pages.map((pageNumber, index) => {
                const isCurrentPage = currentPage === pageNumber;
                const button = createAndSetupButton(
                    pageNumber, isCurrentPage ? 'active' : '', false, onPageButtonClick
                );

                if (isCurrentPage) {
                    currentPageBtn = button;
                }

                buttons.set(button, onPageButtonUpdate(index));
            });

            buttons.set(
                createAndSetupButton('next', 'next-page', disabled.next(), () => currentPage += 1),
                (btn) => btn.disabled = disabled.next()
            )

            buttons.set(
                createAndSetupButton('end', 'end-page', disabled.end(), () => currentPage = totalPages),
                (btn) => btn.disabled = disabled.end()
            )

            buttons.forEach((_, btn) => frag.appendChild(btn));
            paginationButtonContainer.appendChild(frag);

            this.render = (container = document.getElementById("datable_1_paginate")) => {
                 //remove all existing child nodes from parent node
                 while (container.firstChild) {
                    container.removeChild(container.firstChild);
                }
                container.appendChild(paginationButtonContainer);
            }

            this.update = (newPageNumber = currentPage) => {
                //console.log('in update ' +currentPage)
                currentPage = newPageNumber;
                pages = pageNumbers(totalPages, maxPagesVisible, currentPage);
                buttons.forEach((updateButton, btn) => updateButton(btn));
            }

            this.onChange = (handler) => {
                paginationButtonContainer.addEventListener('change', handler);
            }

        }

        // ********************************************************************************************************
        function getPageCount(filter, outofStockState, onlineAvailableState) {
            // get stock count 
            $.ajax({
                url: 'http://app360dev-001-site5.dtempurl.com/api/ECommerce/Admin/EcomProduct/StocksDetails?productCode='+'<%=pro_code%>'+'&nextPage=1&filter='+filter+'&rowCount=12&outofStock='+outofStockState+'&onlineAvailable='+onlineAvailableState+'',
                type: 'GET',
                headers: {
                    Authorization: 'Bearer ' + '<%=current_user.auth.token%>'
                },
                success: function (res) {
                    if(res.content != null){
                        var page_count = Math.ceil(res.content.TotalCount / 12);
                        //console.log('second page_count ' + page_count);
                        if (page_count <= 5) {
                            const paginationButtons = new PaginationButton(page_count, page_count);
                            paginationButtons.render();
                        } else {
                            const paginationButtons = new PaginationButton(page_count, 5);
                            paginationButtons.render();
                        }
                    }else{
                        document.getElementById('search_list').innerHTML = '<tr><td></td><td></td><td><center><span style="color:gray; font-size:18px; font-family: Trirong;">No Stocks Available...</span></center></td><td></td><td></td><td></td></tr>';
                    }
                   
                }
            });
        }
        // Pagination End -------------------------------------------------------------------------------------------
        var filter = 0;
        var onlineAvailableState = false, outofStockState = false;

        function applyFilterations(){
            $('#check_allstock').click(function() {
                document.getElementById('check_allstock').checked = true;
                document.getElementById('check_outofstock').checked = false;
                document.getElementById('check_onlineavailable').checked = false;
                document.getElementById('check_instock').checked = false;
                document.getElementById('check_offline').checked = false;
                filter = 0;
                getPageCount(filter, outofStockState, onlineAvailableState) 
            });
            if (document.getElementById('check_outofstock').checked && document.getElementById('check_onlineavailable').checked){
                filter = 3;
                outofStockState = true
                onlineAvailableState = true;     
            }
            else if (document.getElementById('check_instock').checked && document.getElementById('check_offline').checked){
                document.getElementById('check_allstock').checked = false;
                filter = 3;
                outofStockState = false;    
                onlineAvailableState = false;             
            }
            else if (document.getElementById('check_outofstock').checked){
                document.getElementById('check_allstock').checked = false;
                filter = 1;
                outofStockState = true
            }
            else if (document.getElementById('check_onlineavailable').checked){
                document.getElementById('check_allstock').checked = false;
                filter = 2;
                onlineAvailableState = true;                
            }
            else if (document.getElementById('check_instock').checked){
                document.getElementById('check_allstock').checked = false;
                filter = 1;
                outofStockState = false;    
                onlineAvailableState = false;             
            }
            else if (document.getElementById('check_offline').checked){
                document.getElementById('check_allstock').checked = false;
                filter = 2;
                outofStockState = false 
                onlineAvailableState = false;                     
            }
            getPageCount(filter, outofStockState, onlineAvailableState) 
        }
       
        function filterStocks(page) {            
           var pro_code = '<%=pro_code%>'
           //console.log('fifth: ' + pro_code + " / "+filter + " / "+onlineAvailableState)
           var stockInnerHtml = '';
           $.ajax({
                url: 'http://app360dev-001-site5.dtempurl.com/api/ECommerce/Admin/EcomProduct/StocksDetails?productCode='+pro_code+'&nextPage='+page+'&filter='+filter+'&rowCount=12&outofStock='+outofStockState+'&onlineAvailable='+onlineAvailableState+'',
                type: 'GET',
                headers: {
                    Authorization: 'Bearer ' + '<%=current_user.auth.token%>'
                },
                success: function (res) {
                    if(res.status != "Fail"){ 
                        //console.log(res.content.List.length)
                        var stockObjects = res.content.List;                        
                        if (stockObjects != null) {                           
                            stockObjects.forEach(stock => {
                                var badge_clr = "";
                                var badge_text = "";
                                var hidden = "";
                                var style_class = ""
                                var state = "unchecked"
                                var from_date = "not available", to_date = "not available";

                                if (stock.OnlineAvailable == true) {
                                    badge_clr = "success";
                                    badge_text = "Show";
                                    style_class = "badge badge-sm badge-outline badge-success"
                                    state = "checked"
                                } else {
                                    hidden = 'hidden';
                                    badge_clr = "danger";
                                    badge_text = "Hide";
                                }

                                var exp_date = "";
                                if(stock.ExpiryDate != null){
                                    exp_date = moment.utc(stock.ExpiryDate).local().format('YYYY-MM-DD HH:mm a')                                
                                }
                                if(stock.DiscountFrom != ""){
                                    from_date = moment.utc(stock.DiscountFrom).local().format('YYYY-MM-DD HH:mm a')
                                }
                                if(stock.DiscountTo != ""){
                                    to_date = moment.utc(stock.DiscountTo).local().format('YYYY-MM-DD HH:mm a')
                                }

                                const price = stock.UnitSellingPrice;
                                const formattedPrice = price.toLocaleString('en-US', { minimumFractionDigits: 2, maximumFractionDigits: 2 });

                                stockInnerHtml = stockInnerHtml +
                                '<tr>' +
								'<td>'+
                                '<div class="media align-items-center">'+
								'<div class="media-head me-2">'+
                                                         
								'</div>'+
								'<div class="media-body">'+
								'<div class="text-high-em truncate-text">'+stock.ProductName +
                                '<br><span class="'+style_class+' badge-wth-indicator badge-wth-icon ms-3 d-lg-inline-block d-none" style="margin: 0px !important;"><span '+hidden+'>' +
                                '<i class="badge-dot ri-checkbox-blank-circle-fill"></i>Online Available</span></span></div>' +
								'<div class="fs-7"><label style="width: 100px;">Stock Code</label><label style= "color: #007d88;" class="table-link-text link-medium-em">'+stock.EcomStockCode+'</label></div>'+ 
                                '<div class="fs-7"><label style="width: 100px;">GRN Code</label><label style= "color: #007d88;" class="table-link-text link-medium-em">'+stock.GRNItemCode+'</label></div>'+ 
                                '</div>'+
								'</div>'+
                                '</td>' +
								'<td>LKR ' + formattedPrice + '</td>' +
                                '<td>' + stock.CurrentStock + '</td>' +
                                '<td>' + exp_date + '</td>' +
                                '<td>' +
                                // '<div class="fs-7"><label style="width: 85px;">Discount : </label><label style= "color: #007d88;" class="table-link-text link-medium-em">'+stock.Discount+'</label></div>'+ 
                                '<div class="fs-7"><label style="width: 85px;">Percentage : </label><label style= "color: #007d88;" class="table-link-text link-medium-em">'+stock.DiscountPercentage+'%</label></div>'+ 
                                '<div class="fs-7"><label style="width: 85px;">Start Date : </label><label style= "color: #007d88;" class="table-link-text link-medium-em">'+from_date+'</label></div>'+                                  
                                '<div class="fs-7"><label style="width: 85px;">Exp Date : </label><label style= "color: #007d88;" class="table-link-text link-medium-em">'+to_date+'</label></div>'+
                                '</td>' +
                                // '<td>' +
                                // '<div class="text-high-em">'+stock.LocationType+'</div>'+
								// '<div class="fs-7"><label style= "color: #007d88;" class="table-link-text link-medium-em">'+stock.LocationCode +'</label></div>'+                                   
                                // '</td>' +
								'<td>' +
                                '<div class="d-flex flex-shrink-0 align-items-center ms-3">' +
                                '<label class="switch">' +
                                '<input id="onlineToggle" type="checkbox" '+state+' readonly onclick="changeOnlinAvailableState('+stock.Id+','+stock.OnlineAvailable+')"> <span class="slider round"></span>' +
                                '</label>' +
                                '</div>' +
								'</td>' +
								'</tr>';
                                
                            })
                        } 
                    }else{
                        stockInnerHtml = '<tr><td></td><td></td><td><center><span style="color:gray; font-size:18px; font-family: Trirong;">No Stocks Available...</span></center></td><td></td><td></td><td></td></tr>';  
                    }
                    document.getElementById('search_list').innerHTML = stockInnerHtml;
                }
            })
         
        }

        function changeOnlinAvailableState(code, current_status){
            //console.log(code); //console.log(current_status);
            if (current_status == false){
                //console.log('going to set as online available as TRUE/must set the discounts')
                // show discount modal
                $(".modal-body #stock_code").val(code);
                $('#discount_info').modal('show');
            }else if(current_status == true){
                //console.log('going to set as online available as FALSE')
                $.ajax({
                url: 'http://app360dev-001-site5.dtempurl.com/api/ECommerce/Admin/EcomStock/ChangeOnlineAvailable',
                type: 'POST',
                headers: {
                    Authorization: 'Bearer ' + '<%=current_user.auth.token%>'
                },
                data: JSON.stringify({ 
                    "ecomStockCode": "ESTK"+code,
                    "isOnlineAvailable": false,
                    "discount": 0,
                    "discountPercentage": 0,
                    "discountFrom": "",
                    "discountTo": "",
                    "employeeCode": '<%=current_user.employee.code%>' 
                }),
                contentType: "application/json",
                success: function (res) {
                    //console.log("Request succeeded:", res);
                    if(res.status == "Success"){
                        toastr.success("OnlineAvailable status Updated as FALSE.", 'Success',
                        {
                            timeOut: 1000,
                            fadeOut: 1000,
                            onHidden: function () {
                                window.location.href = window.location.pathname;
                            }
                        }
                        );
                    }else{
                        toastr.error( res.message, 'Error' );
                    }
                },
                error: function(xhr, status, error) {
                    //console.log("Request failed:", xhr, status, error);
                }
            })
            }
            
        }

        function convertDateToRequiredFormat(date){
            const dateObj = new Date(date);

            const year = dateObj.getFullYear();
            const month = dateObj.getMonth() + 1;
            const day = dateObj.getDate();
            const hour = dateObj.getHours();
            const minute = dateObj.getMinutes();
            const second = dateObj.getSeconds();
            const millisecond = dateObj.getMilliseconds();

            const formattedDateTime = `${year}-${month.toString().padStart(2, '0')}-${day.toString().padStart(2, '0')} ${hour.toString().padStart(2, '0')}:${minute.toString().padStart(2, '0')}:${second.toString().padStart(2, '0')}.${millisecond.toString().padStart(3, '0')}`;
            return formattedDateTime;
        }

        function clickbtnUpdate() {
            //console.log('update onlineAvailable status')
            var stock_code = $(".modal-body #stock_code").val();
            var discount = $(".modal-body #discount").val();
            var discount_percentage = $(".modal-body #discount_percentage").val();
            var discount_from = $(".modal-body #discount_from").val();
            var discount_to = $(".modal-body #discount_to").val();
            const current_date = new Date().toISOString();

            var date = convertDateToRequiredFormat(current_date)
            var fromDate = convertDateToRequiredFormat(discount_from)
            var toDate = convertDateToRequiredFormat(discount_to)
            if(discount == 0 || discount_percentage == 0){

                $.ajax({
                url: 'http://app360dev-001-site5.dtempurl.com/api/ECommerce/Admin/EcomStock/ChangeOnlineAvailable',
                type: 'POST',
                headers: {
                    Authorization: 'Bearer ' + '<%=current_user.auth.token%>'
                },
                data: JSON.stringify({ 
                    "ecomStockCode": "ESTK"+stock_code,
                    "isOnlineAvailable": true,
                    "discount": 0,
                    "discountPercentage": 0,
                    "discountFrom": "",
                    "discountTo": "",
                    "employeeCode": '<%=current_user.employee.code%>' 
                }),
                contentType: "application/json",
                success: function (res) {
                    //console.log("Request succeeded:", res);
                    if(res.status == "Success"){
                        toastr.success("OnlineAvailable status Updated as TURE.", 'Success',
                        {
                            timeOut: 1000,
                            fadeOut: 1000,
                            onHidden: function () {
                                window.location.href = window.location.pathname;
                            }
                        }
                    );
                    }else{
                        toastr.error( res.message, 'Error' );
                    }
                },
                error: function(xhr, status, error) {
                    //console.log("Request failed:", xhr, status, error);
                }
            })

            } else if(discount_from > discount_to){
                toastr.error("Start Date and Exp Dates are mismatched.", 'Error', 
                {
                    timeOut: 1000,
                    fadeOut: 1000,
                    onHidden: function () {
                        window.location.href = window.location.pathname;
                    }
                });
            } else if(discount_from < date){
                toastr.error("Incorrect Start Date.", 'Error', 
                {
                    timeOut: 1000,
                    fadeOut: 1000,
                    onHidden: function () {
                        window.location.href = window.location.pathname;
                    }
                });
            }else {

            $.ajax({
                url: 'http://app360dev-001-site5.dtempurl.com/api/ECommerce/Admin/EcomStock/ChangeOnlineAvailable',
                type: 'POST',
                headers: {
                    Authorization: 'Bearer ' + '<%=current_user.auth.token%>'
                },
                data: JSON.stringify({ 
                    "ecomStockCode": "ESTK"+stock_code,
                    "isOnlineAvailable": true,
                    "discount": 0,
                    "discountPercentage": discount_percentage,
                    "discountFrom": fromDate,
                    "discountTo": toDate,
                    "employeeCode": '<%=current_user.employee.code%>' 
                }),
                contentType: "application/json",
                success: function (res) {
                    //console.log("Request succeeded:", res);
                    if(res.status == "Success"){
                        toastr.success("OnlineAvailable status Updated as TURE.", 'Success',
                        {
                            timeOut: 1000,
                            fadeOut: 1000,
                            onHidden: function () {
                                window.location.href = window.location.pathname;
                            }
                        }
                    );
                    }else{
                        toastr.error( res.message, 'Error' );
                    }
                },
                error: function(xhr, status, error) {
                    //console.log("Request failed:", xhr, status, error);
                }
            })

            }
        }

        function cancelProcess(){
            $("#onlineToggle").prop('checked', false);
        }
    </script>

